<!doctype html>
<html lang="{{ page.seoLang }}">

	<head>
		{% include "head.twig" %}
		
		{% include basekit.headScript %}
	</head>

	<body class="{{ page.backgroundClass }} {{ site.colorSwatch }}">
	
		{% include "header.twig" %}
		
		{{widget('feature', 'thisunqiuewidgetname', {"header": "Lorem Ipsum", "strapline":"Feature Strapline, probably needs to be longer", "buttonText":"Read more","defaultBgImg": asset('images/feature-bg.jpg'), 'positionTopPercentage': '50','showBtn': true})|raw}}
		
		<div class="page-wrap container">
			
			{% block content %}
			{% endblock %}

			{% include "footer.twig" %}

		</div>
		
		<script type="text/javascript">
			!function() {
				function supportsSVG() {
					return !!document.createElementNS && !! document.createElementNS('http://www.w3.org/2000/svg', 'svg').createSVGRect
				}
				if (supportsSVG()) document.documentElement.className += ' svg'
			}()
		</script>

		{% include basekit.bodyScript %}

	</body>
</html>